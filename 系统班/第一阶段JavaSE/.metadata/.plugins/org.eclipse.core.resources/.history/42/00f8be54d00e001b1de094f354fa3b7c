package com.ujiuye.homework;

public class HomeWork03 {
	public static void main(String[] args) {
		int num = 0;
		int j = 0;
		for(int x = 1000; x<10000;x++) {
			System.out.println(x);
			for(int i =0 ;i<4 ;i++) {
				j = x%10;
				num = num+j*j*j*j;
				j/=10;
			}
			if(num == x) {
				System.out.println(x);
			}
			
		}
	}
}
